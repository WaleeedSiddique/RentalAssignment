@model IEnumerable<Rental>
@{
    ViewData["Title"] = "Rented Cars";
}

<table class="table table-bordered">
    <thead class="thead-dark">
        <tr>
            <th scope="col">No</th>
            <th scope="col">Name</th>
            <th scope="col">Contact</th>
            <th scope="col">Email</th>
            <th scope="col">PickUp Location</th>
            <th scope="col">PickUp</th>
            <th scope="col">DropOff Location</th>
            <th scope="col">DropOff</th>

        </tr>
    </thead>
    @foreach(var rental in Model){
        <tbody>

            <tr>
                <th scope="row">@rental.RentalID</th>
                <td>@rental.RentalName</td>
                <td>@rental.RentalPhone</td>
                <td>@rental.RentalEmail</td>
                <td>@rental.pickupLocation</td>
                <td>@rental.Pickup</td>
                <td>@rental.DropoffLocation</td>                       
                <td>@rental.Dropoff</td>
            </tr>
        </tbody>
    }
</table>
