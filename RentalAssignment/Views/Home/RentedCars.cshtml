@model List<Vehicle>

<h1>Rented Cars</h1>

@if (Model.Count > 0)
{
    <table>
        <thead>
            <tr>
                <th>Car ID</th>
                <th>Car Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var vehicle in Model)
            {
                <tr>
                    <td>@vehicle.VehicleId</td>
                    <td>@vehicle.VehicleModel</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No Rented cars.</p>
}